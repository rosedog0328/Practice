public class practice0522
{
	public static void main(String args[])
	{
		Rect desk=new Rect(20,20);
		desk.show();
	}
}
class Rect
{
	int width;
	int height;
	public Rect()
	{
		width=10;
		height=30;
	}
	public Rect(int w,int h)
	{
		width=w;
		height=h;
	}
	void show()
	{
		System.out.println("Area= "+ width*height);
		System.out.println("Perimeter= "+(2*(width+height)));
	}
}